{
  "name": "subschema-styles-bootstrap",
  "version": "4.0.0-b-1",
  "main": "lib",
  "description": "Bootstrap CSS support for subschema",
  "scripts": {
    "prepublish": "mrbuilder-webpack",
    "clean": "mrbuilder-clean"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "subschema-transitions": "^4.0.0-b-1",
    "subschema-utils": "^4.0.0-b-1",
    "mrbuilder-plugin-less": "^0.0.4"
  },
  "devDependencies": {
    "mrbuilder": "^0.0.4"
  },
  "source": "src",
  "mrbuilder": {
    "plugins": [
      "mrbuilder-plugin-filetypes",
      "mrbuilder-plugin-fonts",
      [
        "mrbuilder-plugin-css",
        {
          "useNameHash": false
        }
      ],
      "mrbuilder-plugin-less"
    ]
  }
}
