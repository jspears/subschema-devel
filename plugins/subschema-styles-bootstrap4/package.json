{
  "name": "subschema-styles-bootstrap4",
  "version": "4.0.1",
  "main": "lib",
  "description": "Bootstrap4 CSS support for subschema",
  "scripts": {
    "prepublish": "NODE_ENV=development subschema-webpack",
    "clean": "subschema-clean"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "@mrbuilder/plugin-sass": "^4.0.0-alpha.0",
    "subschema-transitions": "^4.0.1",
    "subschema-utils": "^4.0.1"
  },
  "devDependencies": {
    "subschema-dev-support": "^4.0.1"
  },
  "source": "src",
  "mrbuilder": {
    "plugins": [
      "@mrbuilder/plugin-filetypes",
      "@mrbuilder/plugin-fonts",
      [
        "@mrbuilder/plugin-css",
        {
          "useNameHash": false
        }
      ],
      "@mrbuilder/plugin-sass"
    ]
  },
  "gitHead": "7102e6249fb7d12545737ed895b0d269b401f9db",
  "license": "MIT"
}