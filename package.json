    "prepublish": "lerna run prepublish",
    "demo": "cd subschema-component-demo && yarn run demo"
  },
  "private": true,
  "devDependencies": {
    "lerna": "^2.5.1",
    "lerna-relinker": "^1.5.0",
    "rimraf": "^2.6.2"
