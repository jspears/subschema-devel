  "workspaces": [
    "presets/*",
    "plugins/*",
    "examples/*"
    "resolvers/*",
    "core/*",
    "devtools/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "yarn": "^1.3.0"
  }
