{
  "dependencies": {
    "istanbul": "^0.4.5",
    "nyc": "^11.3.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0"
  },
  "scripts": {
    "clean": "lerna clean --yes && lerna run clean",
    "rebuild": "lerna clean --yes && lerna run clean && yarn install && lerna run prepublish && lr-idea",
