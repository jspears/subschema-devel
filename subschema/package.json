{
  "name": "subschema",
  "version": "4.0.0-b-1",
  "description": "Subschema",
  "main": "lib",
  "scripts": {
    "compile-default-loader": "subschema-autoloader | subschema-babel --out-file  ./lib/DefaultLoader.js",
    "compile-lib": "subschema-babel && yarn run compile-default-loader",
    "compile-server": "subschema-webpack --output-path lib --no-style-loader --target node --output-filename index.js --use-externals react,react-dom,fbjs",
    "compile-web": "NODE_ENV=production SUBSCHEMA_USE_ALIASES=react,react-dom subschema-webpack --devtool sourcemap -p  --output-path dist --no-externalize-peers --output-filename subschema.js",
    "compile-web-debug": "subschema-webpack --no-externalize-peers --devtool inline-source-map --output-path dist --output-filename subschema-debug.js",
    "compile-web-noreact": "subschema-webpack -p  --output-path dist --output-filename subschema-noreact.js",
    "karma": "subschema-karma",
    "test": "subschema-karma",
    "prepublish": "yarn run compile-lib && yarn run compile-web && yarn run compile-web-debug && yarn run compile-web-noreact",
    "clean": "subschema-clean"
  },
  "peerDependencies": {
    "react": "16.x || ^15.5.0",
    "react-dom": "16.x || ^15.5.0"
  },
  "dependencies": {
    "subschema-preset-web-form": "^4.0.0-b-1",
    "subschema-preset-resolvers": "^4.0.0-b-1",
    "subschema-context": "^4.0.0-b-1",
    "subschema-dev-autoloader": "^4.0.0-b-1"
  },
  "devDependencies": {
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "subschema-dev-support": "^4.0.0-b-1",
    "subschema-test-support": "^4.0.0-b-1",
    "subschema-dev-utils": "^4.0.0-b-1"
  },
  "subschema": {
    "webpack": "./subschema-webpack.config",
    "options": {
      "loader": false
    },
    "plugins": [
      [
        "subschema-dev-autoloader",
        {
          "loader": false
        }
      ]
    ],
    "presets": [
      "subschema-preset-web-form",
      "subschema-preset-resolvers"
    ]
  },
  "source": "src"
}