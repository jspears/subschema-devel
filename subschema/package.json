{
  "name": "subschema",
  "version": "4.0.0-b-1",
  "description": "Subschema",
  "main": "lib",
  "scripts": {
    "compile-default-loader": "subschema-autoloader | mrbuilder-babel --out-file  ./lib/DefaultLoader.js && subschema-importer | mrbuilder-babel --out-file ./lib/importer.js",
    "compile-lib": "mrbuilder-babel && yarn run compile-default-loader",
    "compile-server": "mrbuilder-webpack --output-path lib --no-style-loader --target node --output-filename index.js --use-externals react,react-dom,fbjs",
    "compile-web": "NODE_ENV=production SUBSCHEMA_USE_ALIASES=react,react-dom mrbuilder-webpack --devtool sourcemap -p  --output-path dist --no-externalize-peers --output-filename subschema.js",
    "compile-web-debug": "mrbuilder-webpack --no-externalize-peers --devtool inline-source-map --output-path dist --output-filename subschema-debug.js",
    "compile-web-noreact": "mrbuilder-webpack -p  --output-path dist --output-filename subschema-noreact.js",
    "karma": "mrbuilder-karma",
    "test": "mrbuilder-karma",
    "prepublish": "yarn run compile-lib && yarn run compile-web && yarn run compile-web-debug && yarn run compile-web-noreact",
    "clean": "mrbuilder-clean"
  },
  "peerDependencies": {
    "react": "16.x || ^15.5.0",
    "react-dom": "16.x || ^15.5.0"
  },
  "dependencies": {
    "subschema-preset-web-form": "^4.0.0-b-1",
    "subschema-preset-resolvers": "^4.0.0-b-1",
    "subschema-context": "^4.0.0-b-1",
    "subschema-plugin-autoloader": "^4.0.0-b-1",
    "mrbuilder": "^0.0.5"
  },
  "devDependencies": {
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "subschema-test-support": "^4.0.0-b-1"
  },
  "mrbuilder": {
    "plugins": [
      [
        "mrbuilder-plugin-html",
        false
      ],
      "subschema-plugin-autoloader",
      "./subschema-webpack.config"
    ]
  },
  "subschema": {
    "options": {
      "loader": false
    },
    "plugins": [
      [
        "subschema-plugin-autoloader"
      ]
    ],
    "presets": [
      "subschema-preset-web-form",
      "subschema-preset-resolvers"
    ]
  },
  "source": "src"
}
