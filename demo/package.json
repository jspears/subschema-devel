{
  "name": "subschema-demo",
  "version": "4.0.0-b-1",
  "dependencies": {
    "subschema": "^4.0.0-b-1",
    "subschema-preset-examples": "^4.0.0-b-1",
    "subschema-preset-resolvers": "^4.0.0-b-1",
    "subschema-preset-web-form": "^4.0.0-b-1",
    "subschema-plugin-demo": "^4.0.0-b-1",
    "subschema.wiki": "^4.0.0-b-1"
  },
  "devDependencies": {
    "mrbuilder": "^0.0.2"
  },
  "scripts": {
    "start": "mrbuilder-webpack-dev-server",
    "demo": "SUBSCHEMA_USE_ANALYTICS='UA-72459938-1' mrbuilder-webpack --demo ../docs",
    "server": "mrbuilder-webpack-dev-server"
  },
  "subschema": {
    "plugins": [
      [
        "subschema",
        {
          "loader": false
        }
      ],
      "subschema-plugin-demo",
      [
        "subschema.wiki",
        {
          "loader": "Doc"
        }
      ]
    ],
    "presets": [
      "subschema-preset-examples",
      "subschema-preset-resolvers",
      "subschema-preset-web-form"
    ]
  },
  "source": "src",
  "main": "lib"
}
